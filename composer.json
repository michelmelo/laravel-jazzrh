{
    "name": "michelmelo/laravel-jazzrh",
    "description": "A comprehensive HR management package for Laravel 12",
    "type": "library",
    "version": "1.0.0",
    "license": "MIT",
    "authors": [
        {
            "name": "Michel Melo",
            "email": "dev@michelmelo.com"
        }
    ],
    "require": {
        "php": "^8.3",
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5",
        "laravel/pint": "^1.13",
        "orchestra/testbench": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "MichelMelo\\JazzRh\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MichelMelo\\JazzRh\\Tests\\": "tests/",
            "Database\\Factories\\": "database/Factories/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "MichelMelo\\JazzRh\\Providers\\JazzRhServiceProvider"
            ],
            "aliases": {
                "JazzRh": "MichelMelo\\JazzRh\\Facades\\JazzRh"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    }
}
